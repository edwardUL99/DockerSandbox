      "user": "sandbox",
      "working-directory": "/home/sandbox",
      "limits": {
        "cpuCount": 1,
        "memory": 64
      },
      "networkDisabled": true
    },
    {
      "name": "gcc_compile",
      "image": "gcc-docker",
      "container-name": "gcc-docker",
      "user": "root",
      "working-directory": "/home/sandbox",
      "networkDisabled": true
    },
    {
      "name": "gcc_run",
      "image": "gcc-docker",
      "container-name": "gcc-docker",
      "user": "sandbox",
      "working-directory": "/home/sandbox",
      "networkDisabled": true
    },
    {
      "name": "java_compile",
      "image": "gcc-java",
      "container-name": "gcc-java",
      "user": "root",
      "working-directory": "/home/sandbox",
      "networkDisabled": true
    },
    {
      "name": "java_run",
      "image": "gcc-java",
      "container-name": "gcc-java",
      "user": "sandbox",
      "working-directory": "/home/sandbox",
      "networkDisabled": true
    }
